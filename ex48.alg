algoritmo "semnome"
// Função :
// Autor :
// Data : 27/10/2024
// Seção de Declarações 
var
CONT, SOMA, N: INTEIRO
inicio
// Seção de Comandos 
cont <- 1
Soma <- 0
Enquanto (Cont <= 7) faca
    Escreva("Digite o ", cont, "º número: ")
    Leia(N)
    soma <- Soma + N
    cont <- Cont + 1
FimEnquanto
Escreva("A soma entre os números é ", soma)
fimalgoritmo
